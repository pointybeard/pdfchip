OUTPUT;

$showHelp = (true == in_array('--help', $argv));
$showStatus = (true == in_array('--status', $argv));
$showVersion = (true == in_array('--version', $argv));

// first item is always the path to this file
unset($argv[0]);

// reset the array indexes
$argv = array_values($argv);

// strip out all options from $argv since we ignore them anyway
for($ii = 0; $ii < count($argv); $ii++) {
    if($argv[$ii][0] == '-' && $argv[$ii][1] == '-') {
        unset($argv[$ii]);
    }
}

// no input or output specified, so we'll show the help screen instead
if(false == $showStatus && false == $showVersion && true == empty($argv)) {
    $showHelp = true;
}

// --help
if (true == $showHelp) {
    echo 'mock pdfChip help

Usage:
	pdfChip <input file> [<input file> [...] ] <output_file>

Purpose:
	Convert HTML to PDF

Arguments:
	<input file>: HTML or SVG input file
	<output file>: PDF output file

Options:
	all options are ignored

Further commands:
	--help: Display help
	--version: Display version information
	--status: Display license status'.PHP_EOL;
    exit;
}

// --status
if (true == $showStatus) {
    echo 'mock pdfChip status

License details:
	Product: callas pdfChip S
	License: Full
	ActivationCode: XRQKF8K7E3HF8K474HA7E3QKJJNKEVXFRK7E3HF8K47
	S/N: 100010716
	Name: Mock Person
	Company: Mock Company
	EMail: mock@example.com

	Activation: XRQKF8K7E3HF8K474HA7E3QKJJNKEVXFRK7E3HF8K47	callas pdfChip S	/home/mock/.callas software/callas pdfChip mock/License.txt

Limitations:
	Parallel processes: 1
	Barcodes: EAN, UPC, ISBN, Code39, Code 128 and QR
	Pages per document: 25
	Pages per hour: 1000 (1000 remaining)
	Advanced pagination: No

Return codes:
20: An unknown error has occurred.
21: Unable to write pdf file, file access error.
...
111: OCG or OCMD error.
112: Zero page size specified in CSS.
113: Conversion did not finish during user specified timeout.'.PHP_EOL;
    exit;
}

// --version
if (true == $showVersion) {
  echo 'callas pdfChip: 2.3.070 (x64)
cChipPDF-Library: 2.14.088
Qt: 5.6.3'.PHP_EOL;
  exit;
}

function finish(int $code = 0) {
    echo 'Time elapsed: '.rand(1,999).' msec msec'.PHP_EOL;
    exit($code);
}

$inputFiles = [];

// we always assume there is at least a single input file
$inputFiles[] = array_shift($argv);

// of what is left, the output file is always the last item
$outputFile = array_pop($argv);

// (guard) no output file specified so construct one from the input file name
if(true == empty($outputFile)) {
    $outputFile = pathinfo($inputFiles[0], PATHINFO_FILENAME) . '.pdf';
}

// everything left in $argv is an additional input file
$inputFiles = array_merge($inputFiles, $argv);

/*
|--------------------------------------------------------------------------
| Start emulating output to console
|--------------------------------------------------------------------------
|
| This section will attempt to accuately emulate the output to the console
| as each file is loaded, read, and output is attempted. Even though
| the output is static, we'll still check each file exists and
| throw an error accordingly
|
*/

foreach($inputFiles as $index => $file) {
    echo 'converting "'.$file.'"'.PHP_EOL;

    // pdfChip includes an additional line for the first item
    if(0 == $index) {
        echo 'output PDF "'.$outputFile.'"'.PHP_EOL;
    }

    // (guard) input file is not readable
    if(false == file_exists($file)) {
        echo 'Error: Failed to load page: "'.$file.'"'.PHP_EOL;
        finish(1);
    }

    echo "page {$index}
end page {$index}".PHP_EOL;
}

// (guard) outputFile path is not writable
if (false == is_writable(dirname($outputFile))) {
    echo 'PdfCreator Error: Unable to write pdf file, file access error.'.PHP_EOL;
    finish(1);
}

// Write the mock output to outputFile path
file_put_contents($outputFile, base64_decode($output));

finish();
